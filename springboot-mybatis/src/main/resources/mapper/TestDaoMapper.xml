<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.spring.boot.mybatis.mybatis.dao.TestDao">
    <!-- 根据主键查询-->
    <select id="getById" resultType="com.spring.boot.mybatis.mybatis.entity.TestEntity" parameterType="java.lang.Integer">
        select  *
        from test
        where id = #{id}
    </select>
</mapper>